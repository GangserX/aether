# Aether Orchestrate - Staging Environment Variables
# Copy this file to .env.staging and fill in the values

# Application
NODE_ENV=staging
PORT=8080

# Database (PostgreSQL on RDS)
DATABASE_URL=postgresql://aether_admin:PASSWORD@staging-rds-endpoint:5432/aether_db

# Redis (ElastiCache)
REDIS_HOST=staging-redis-endpoint
REDIS_PORT=6379

# JWT Authentication
JWT_SECRET=your-staging-jwt-secret-min-32-chars

# AI Services
OPENROUTER_API_KEY=your-openrouter-api-key
TAVILY_API_KEY=your-tavily-api-key
GROQ_API_KEY=your-groq-api-key

# Email Service
RESEND_API_KEY=your-resend-api-key

# Slack Integration
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token

# OAuth - GitHub
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret

# OAuth - Google
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# AWS Configuration
AWS_REGION=eu-north-1

# Frontend URL (for CORS)
FRONTEND_URL=https://staging.yourdomain.com

# API URL
API_URL=https://api-staging.yourdomain.com
